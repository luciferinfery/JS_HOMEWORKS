{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "func-names": "off",
        "no-console": "off",
        "no-continue": "off",
        "no-restricted-syntax": ["error", "WithStatement"],
        "no-labels": "off"
    }
}
